
# 002 read and write standby

record(bi,"$(P)$(M)readStandby")
	{
		field(DESC, "read Standby")
		field(DTYP, "stream")
		field(INP, "@TC110.proto readStandby $(PORT)")
		field(ZNAM, "off")
		field(ONAM, "on")
		field(SCAN, "1 second")
	}
	
record(bo,"$(P)$(M)setStandby")
	{
		field(DESC, "set Standby")
		field(DTYP, "stream")
		field(OUT, "@TC110.proto setStandby $(PORT)")
		field(ZNAM, "off")
		field(ONAM, "on")
	}

