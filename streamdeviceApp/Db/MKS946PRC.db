record(ai,"$(P)$(PRC):readpressure")
  {
	field(DESC,"Pressure")
	field(SCAN,"1 second")
	field(DTYP,"stream")
	field(INP,"@MKS946PRC.proto readpressure($(N)) $(PORT)")
	field(EGU,"mbar")
	field(HOPR,"1.00E-2")
	field(LOPR,"1.00E-11")
	field(PREC,"1")
	field(FLNK, "$(P)$(PRC):STA")
	field(PINI,"YES")
  }

record(mbbi, "$(P)$(PRC):STA") 
  {
    field(DTYP, "Soft Channel")
    field(DESC, "Gauge Status")
    field(ZRST, "OK")
    field(ONST, "OK")
    field(TWST, "Above Range")
    field(THST, "At Atmosphere")
    field(FRST, "Low Emission")
    field(FVST, "Filament Off")
    field(SXST, "HV Off")
    field(SVST, "Startup Delay")
    field(EIST, "Below Range")
    field(NIST, "Controlled")
    field(TEST, "Protected State")
    field(ELST, "No Gauge")
    field(TVST, "Not Connected")
    field(TTST, "Wrong Gauge")
    field(FTST, "Bad Command")
    field(FFST, "Locked Out")
    field(UNSV, "MAJOR")
    field(ZRSV, "NO_ALARM")
    field(ONSV, "NO_ALARM")
    field(TWSV, "MINOR")
    field(THSV, "MINOR")
    field(FRSV, "MAJOR")
    field(FVSV, "MAJOR")
    field(SXSV, "MAJOR")
    field(SVSV, "MINOR")
    field(EISV, "NO_ALARM")
    field(NISV, "MINOR")
    field(TESV, "MINOR")
    field(ELSV, "MAJOR")
    field(TVSV, "MAJOR")
    field(TTSV, "MAJOR")
    field(FTSV, "MAJOR")
    field(FFSV, "MAJOR")
}