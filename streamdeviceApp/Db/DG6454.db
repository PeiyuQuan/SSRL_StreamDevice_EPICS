
#Delay
record (mbbo,"$(P)$(K):setchannel")
	{
		field(DESC,"Delay")
		field(DTYP,"stream")
		field(OUT,"@DG6454.proto setDlay($(N),$(P)$(K):setdelaytime) $(PORT)")
		field(ZRVL,"0")
		field(ONVL,"1")
		field(TWVL,"2")
		field(THVL,"3")
		field(FRVL,"4")
		field(FVVL,"5")
		field(SXVL,"6")
		field(SVVL,"7")
		field(EIVL,"8")
		field(NIVL,"9")
		field(ZRST, "T0")
		field(ONST, "T1")
		field(TWST, "A")
		field(THST, "B")
		field(FRST, "C")
		field(FVST, "D")
		field(SXST, "E")
		field(SVST, "F")
		field(EIST, "G")
		field(NIST, "H")	
	}
	
record(ao,"$(P)$(K):setdelaytime")
	{
	field(DESC,"Delay time")
	}



record (mbbi,"$(P)$(K):getchannel")
	{
		field(DESC,"Delay")
		field(DTYP,"stream")
		field(INP,"@DG6454.proto getDlay($(N),$(P)$(K):getdelaytime) $(PORT)")
		field(ZRVL,"0")
		field(ONVL,"1")
		field(TWVL,"2")
		field(THVL,"3")
		field(FRVL,"4")
		field(FVVL,"5")
		field(SXVL,"6")
		field(SVVL,"7")
		field(EIVL,"8")
		field(NIVL,"9")
		field(ZRST, "T0")
		field(ONST, "T1")
		field(TWST, "A")
		field(THST, "B")
		field(FRST, "C")
		field(FVST, "D")
		field(SXST, "E")
		field(SVST, "F")
		field(EIST, "G")
		field(NIST, "H")
		field(SCAN, "1 second")	
	}
	
record(ai,"$(P)$(K):getdelaytime")
	{
	field(DESC,"Delay time")
	}


