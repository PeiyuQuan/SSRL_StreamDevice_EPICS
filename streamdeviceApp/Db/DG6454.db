record(bo,$(P)$(K):setSPDL)
	{
		field(DESC,"Step delay")
		field(DTYP,"stream")
		field(OUT,"@DG6454.proto setSPDL($(N)) $(PORT)")
		field(ZNAM,"down")
		field(ONAM,"up")
	}
	
###############################################################	
record(ao,$(P)$(K):setSSDL)
	{
		field(DESC,"step Size Delay")
		field(DTYP,"stream")
		field(OUT,"@DG6454.proto setSSDL($(N)) $(PORT)")
	}


record(ai,$(P)$(K):getSSDL)
	{
		field(DESC,"step Size Delay")
		field(DTYP,"stream")
		field(INP,"@DG6454.proto getSSDL($(N)) $(PORT)")
		field(SCAN, "1 second")
	}

