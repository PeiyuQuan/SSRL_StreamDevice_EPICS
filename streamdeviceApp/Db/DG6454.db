record(bo,$(P)setCLS)
	{
		field(DESC,"clear status")
		field(DTYP,"stream")
		field(OUT,"@DG6454.proto setCLS $(PORT)")
		field(ZNAM,"No")
		field(ONAM,"Yes")
	}
	

