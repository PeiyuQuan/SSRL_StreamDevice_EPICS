
record(longout,$(P)$(L):setPRES)
	{
		field(DESC,"set prescale factor")
		field(DTYP,"stream")
		field(OUT,"@DG6453.proto setPRES($(N)) $(PORT)")
	}


record(longin,$(P)$(L):getPRES)
	{
		field(DESC,"set prescale factor")
		field(DTYP,"stream")
		field(INP,"@DG6453.proto getPRES($(N)) $(PORT)")
		field(SCAN, "1 second")
	}


