record(mbbi,$(P)$(MFC):getmode)
	{
	field(DTYP, "stream")
      	field(INP, "@MKS946MFC.proto getmode($(N)) $(PORT)")
      	field(ZRVL, "0")
      	field(ZRST, "OPEN")
      	field(ONVL, "1")
      	field(ONST, "CLOSE")
      	field(TWVL, "2")
      	field(TWST,"SETPONTS")
      	field(THVL, "3")
      	field(THST,"PCTRL")
      	field(FRVL, "4")
      	field(FRST,"RATIO")
      	field(FVVL, "5")
      	field(FVST,"PRESET")
	field(DESC,"read mode")
	field(SCAN,"1 second")
	}

