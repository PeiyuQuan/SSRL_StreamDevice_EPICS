
record(ai,$(P)$(MFC):readflow)
	{
	  field(DTYP,"stream")
	  field(DESC,"read flow rate")
	  field(INP,"@MKS946MFC.proto readflow($(N)) $(PORT)")
	  field(SCAN,"1 second")
	  field(EGU,"Sccm")
	}
	

	
record(ai,$(P)$(MFC):readscalefactor)
	{
	  field(DTYP,"stream")
	  field(INP,"@MKS946MFC.proto readscalefactor($(N)) $(PORT)")
	  field(PREC, "1")
	  field(HOPR, "10")
      field(LOPR, "0.1")
      field(SCAN,"1 second")
	  field(DESC,"read scale factor")
	}
	
