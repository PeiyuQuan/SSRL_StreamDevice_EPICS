
record(ai,"$(P)$(M)readpressure")
	{
		field(DESC,"read Pressure")
		field(DTYP,"stream")
		field(INP,"@gauge354.proto readpressure $(PORT)")
		field(EGU,"Torr")
		field(PREC,"2")
		field(SCAN,"1 second")
	}

	
	
